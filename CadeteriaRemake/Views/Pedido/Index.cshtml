@model List<CadeteriaRemake.ViewModels.PedidoViewModel>
    <div class="row">
        <div class="col-md-10 mx-auto">
            <table class="table table-striped">
                <tr>
                    <th>Id</th>
                    <th>Observacion</th>
                    <th>Tipo</th>
                    <th>Estado</th>
                    <th>Cupon</th>
                    <th>Cliente</th>
                    <th>Cadete</th>
                    <th>Costo</th>
                </tr>
                @foreach (var pedido in Model)
                {
                    <tr>
                        <td>@pedido.numero</td>
                        <td>@pedido.observacion</td>
                        <td>@pedido.tipoPedido</td>
                        <td>@pedido.estado_pedido</td>
                        <td>@pedido.cupon</td>
                        <td>@pedido.cliente.Nombre</td>
                        <td>@pedido.cadete.Nombre</td>
                        <td>@pedido.costo</td>
                    </tr>
                }
            </table>
        </div>
    </div>

