@model List<CadeteriaRemake.ViewModels.PedidoViewModel>
<div class="row">
    <div class="col-md-10 mx-auto">
        <table class="table table-striped">
            <tr>
                <th>Cadete</th>
                <th>Jornal</th>
            </tr>
            @foreach (var pedido in Model)
            {
                <tr>
                    <td>@pedido.cadete.Nombre</td>
                    <td>@pedido.costo</td>
                </tr>
            }
        </table>
    </div>
</div>

